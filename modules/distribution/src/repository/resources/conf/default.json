{
  "realm_manager.properties": {
    "isCascadeDeleteEnabled": true,
    "initializeNewClaimManager": true
  },
  "user_store.type": "read_write_ldap",
  "keystore.primary.type" : "JKS",
  "keystore.userstore_password_encryption": "",
  "truststore.type" : "JKS",

  "transport.https.sslHostConfig.properties.certificateVerification": "want",

  "authentication.consent.data_source": "jdbc/WSO2IdentityDB",

  "server.hide_menu_items": [
    "claim_mgt_menu",
    "remote_fetch_providers_menu",
    "identity_mgt_emailtemplate_menu",
    "identity_security_questions_menu",
    "metrics_menu",
    "shutdown_restart_menu"
  ],
  "http_access_log.pattern": "%h %l %u %t %r %s %b &quot;%{Referer}i&quot; &quot;%{User-Agent}i&quot; %T",

  "log_correlation.header_config": "{'activityid':'Correlation-ID'}",

  "monitoring.jmx.rmi_server_start": false,
  "product.version" : "${product.version}",

  "transport.http.enabled" : true,
  "transport.https.enabled" : true,

  "system.parameter.\u0027org.wso2.CipherTransformation\u0027": "AES/GCM/NoPadding",

  "encryption.internal_crypto_provider": "org.wso2.carbon.crypto.provider.SymmetricKeyInternalCryptoProvider",
  "encryption.key": "03BAFEB27A8E871CAD83C5CD4E771DAB",

  "authorization_manager.properties.GroupAndRoleSeparationEnabled": "true",
  "authorization_manager.properties.GroupAndRoleSeparationImprovementsEnabled": "true",

  "user_store.properties.UsernameJavaScriptRegEx": "^[\\S]{3,50}$",

  "http_header_security_filter.hsts.enabled": false,
  "http_header_security_filter.anti_click_jacking.enabled": false,
  "http_header_security_filter.anti_click_jacking.hsts.enabled": false,
  "http_header_security_filter.anti_click_jacking.block_content_type_sniffing.enabled": false,
  "http_header_security_filter.anti_click_jacking.xss_protection.enabled": false,
  "http_header_security_filter.anti_click_jacking.urls": [
    "/carbon/*",
    "/services/*",
    "/samlsso/*",
    "/openid/*",
    "/openidserver/*",
    "/passivests/*",
    "/acs/*",
    "/iwa/*",
    "/identity/*",
    "/thriftAuthenticator/*"
  ],

  "tomcat.session_config.error_page.error_400": "/carbon/errors/error_400.html",
  "tomcat.session_config.error_page.error_401": "/carbon/errors/error_401.html",
  "tomcat.session_config.error_page.error_403": "/carbon/errors/error_403.html",
  "tomcat.session_config.error_page.error_404": "/carbon/errors/error_404.html",
  "tomcat.session_config.error_page.error_405": "/carbon/errors/error_405.html",
  "tomcat.session_config.error_page.error_408": "/carbon/errors/error_408.html",
  "tomcat.session_config.error_page.error_410": "/carbon/errors/error_410.html",
  "tomcat.session_config.error_page.error_500": "/carbon/errors/error_500.html",
  "tomcat.session_config.error_page.error_502": "/carbon/errors/error_502.html",
  "tomcat.session_config.error_page.error_503": "/carbon/errors/error_503.html",
  "tomcat.session_config.error_page.error_504": "/carbon/errors/error_504.html",
  "tomcat.session_config.error_page.error_default": "/carbon/errors/error.html",
  "tomcat.session_config.cookie_config.secure" : true,
  "tomcat.session_config.tracking_mode" : "COOKIE"
}
